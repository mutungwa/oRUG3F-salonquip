{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "src/server/libraries/email/internal/templates/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/templates/(.*)",
        "dest": "/src/server/libraries/email/internal/templates/$1"
      }
    ]
  }