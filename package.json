    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@zenstackhq/runtime": "2.2.4",
    "@zenstackhq/server": "2.2.4",
    "@zenstackhq/tanstack-query": "2.2.4",
    "@zenstackhq/trpc": "2.2.4",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "nodemon": "^3.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "zenstack": "2.2.4"
  },
