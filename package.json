    "next-auth": "^4.24.7",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.9.14",
    "notistack": "^3.0.1",
    "openai": "^4.52.0",
    "postcss": "^8.4.39",
    "prisma": "5.15.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-error-boundary": "^5.0.0",
    "react-to-print": "^3.0.5",
    "recharts": "^2.15.1",
    "resend": "^4.1.2",
    "sass": "^1.62.1",
    "server-only": "^0.0.1",
    "stripe": "^15.12.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.6",
    "uuid": "^10.0.0",
